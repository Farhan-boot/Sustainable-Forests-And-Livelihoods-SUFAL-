@using PTSL.GENERIC.Web.Core.Model.EntityViewModels.ForestManagement;
@model PTSL.GENERIC.Web.Core.Model.EntityViewModels.ForestManagement.CommitteesConfigurationVM
@{
    ViewBag.Title = "Committee Details";
}

<div class="card">
	<div class="card-header">
		<h4>
            <a href="/CommitteesConfiguration/Index" class="mr-1 back">
				<svg width="20" height="20" viewBox="0 0 32 32">
					<path fill="currentColor" d="m14 26l1.41-1.41L7.83 17H28v-2H7.83l7.58-7.59L14 6L4 16l10 10z" />
				</svg>
			</a>
           Committee Details
		</h4>
	</div>

	<div class="card-body">
		<div class="form-horizontal">
			<div class="form-group">
                <label style="font-size:15px;">
                    Committee Type: <b>@Model?.CommitteeTypeConfiguration?.CommitteeTypeName</b>
                </label>
			</div>
			<div class="form-group">
                <label style="font-size:15px;">Committee Name: <b>@Model.CommitteeName</b></label>
			</div>
		</div>
	</div>
</div>

