@model PTSL.GENERIC.Web.Core.Model.EntityViewModels.GeneralSetup.FinancialYearVM
@{
    ViewBag.Title = "Details";
}

<div class="card">
    <div class="card-header">
        <h4>
            <a href="/FinancialYear/Index" class="mr-1 back">
                <svg width="20" height="20" viewBox="0 0 32 32">
                    <path fill="currentColor" d="m14 26l1.41-1.41L7.83 17H28v-2H7.83l7.58-7.59L14 6L4 16l10 10z" />
                </svg>
            </a>
            Financial Year Details
        </h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label style="font-size:15px;">Id: <b>@Model.Id</b></label>
            </div>
            <div class="form-group">
                <label style="font-size:15px;">English Name: <b>@Model.Name</b></label>
            </div>
            <div class="form-group">
                <label style="font-size:15px;">Bangla Name: <b>@Model.NameBn</b></label>
            </div>
        </div>
    </div>
</div>

