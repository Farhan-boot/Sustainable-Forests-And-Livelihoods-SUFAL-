<style>
	.cflex {
		display: flex;
	}
	.input-select {
		column-gap: 15px;
		padding: 15px 0 5px 0;
		border-top: 1px solid gainsboro;
	}
	.ctab {
		padding: 5px 15px;
		color: black;
		border-radius: 5px 5px 0 0;
		cursor: pointer;
	}
	.ctab.active {
		background: #d1d1d1;
		font-weight: bold;
	}

    .forest-input {
        display: flex;
        gap: 2px;
        align-items: baseline;
    }

        .forest-input label {
            /* background: #abebc6; */
            color: black;
            padding: 7px 12px;
            border-radius: 5px;
            font-weight: 500;
        }

        .forest-input select {
            border: 1px solid #abebc6;
            color: black;
            padding: 7px 12px;
            border-radius: 5px;
            font-weight: 500;
            min-width: 130px;
            max-width: 250px;
        }

    .search {
        margin-left: auto;
    }

        .search button {
			all: unset;
			background: #abebc6;
			padding: 3px 12px;
			font-weight: 500;
			border-radius: 5px;
			color: #212f3d;
			cursor: pointer;
        }

            .search button:hover {
                opacity: 0.9;
            }

    #energy-condition-compact, #event {
        box-shadow: none;
    }

    #energy-condition-compact {
        border: 1px solid rgba(0, 0, 0, 0.15);
        height: 500px;
        overflow: hidden;
        display: flex;
        flex-direction: column;
        justify-content: space-between;
    }

        #energy-condition-compact .chart {
            display: flex;
            flex-direction: row;
            align-items: flex-end;
            justify-content: space-around;
            max-height: 400px;
        }

        #energy-condition-compact .single-chart {
            display: flex;
            flex-direction: column;
            align-items: center;
            max-height: 400px;
        }

            #energy-condition-compact .single-chart .energy-chart-body {
                -webkit-transform: rotate(180deg);
                transform: rotate(180deg);
                -webkit-writing-mode: vertical-rl;
                writing-mode: vertical-rl;
                background: #dcdedf;
                max-height: 400px;
                padding: 21px 10px;
                border-radius: 111.748px;
                font-size: 16px;
            }

                #energy-condition-compact .single-chart .energy-chart-body:hover {
                    cursor: pointer;
                    background: #199669;
                    color: white;
                }

            #energy-condition-compact .single-chart .energy-label {
                margin-top: 5px;
                text-indent: 10px;
            }
</style>

