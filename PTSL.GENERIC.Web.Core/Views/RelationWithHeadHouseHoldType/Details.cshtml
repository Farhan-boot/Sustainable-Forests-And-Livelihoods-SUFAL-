@model PTSL.GENERIC.Web.Core.Model.EntityViewModels.Beneficiary.RelationWithHeadHouseHoldTypeVM
@{
    ViewBag.Title = "Details";
}

<div class="card">
    <div class="card-header">
        <h4>Energy Type Details</h4>
    </div>

    <div class="card-body">
        <div class="form-horizontal">
            <div class="form-group">
                <label style="font-size:15px;">Id: <b>@Model.Id</b></label>
            </div>
            <div class="form-group">
                <label style="font-size:15px;">English Name: <b>@Model.Name</b></label>
            </div>
            <div class="form-group">
                <label style="font-size:15px;">Bangla Name: <b>@Model.NameBn</b></label>
            </div>
        </div>
    </div>
    <hr />
    <div class="card-header pull-left">
        <a href="/RelationWithHeadHouseHoldType/Index" class="btn btn-warning">Return to Index</a>
    </div>
</div>

